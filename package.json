{
  "name": "project-init",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start-mac": "export NODE_ENV=development&& node ./bin/www",
    "start-win": "set NODE_ENV=development&& node ./bin/www",
    "build": "node --max_old_space_size=4000 node_modules/webpack/bin/webpack.js  --progress --profile --colors  --config webpack.prod.js",
    "build:start-mac": "export NODE_ENV=production&& forever start html",
    "build:start-win": "set NODE_ENV=production&& forever start html",
    "build:restart-mac": "export NODE_ENV=production&& forever restart html",
    "build:restart-win": "set NODE_ENV=production&& forever restart html",
    "watch": "webpack --progress --colors --watch  --config webpack.dev.js&&node ./bin/www",
    "watch:start-mac": "export NODE_ENV=watch&&node ./bin/www",
    "watch:start-win": "set NODE_ENV=watch&&node ./bin/www",
    "push": "git push github master",
    "pull": "git fetch github&&git pull github master"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "cookie-parser": "~1.4.3",
    "debug": "~2.6.9",
    "echarts": "^4.2.0-rc.2",
    "ejs": "~2.5.7",
    "express": "~4.16.0",
    "http-errors": "~1.6.2",
    "jquery": "^3.3.1",
    "js-cookie": "^2.2.0",
    "lodash": "^4.17.11",
    "moment": "^2.22.2",
    "morgan": "~1.9.0",
    "mysql": "^2.16.0"
  },
  "devDependencies": {
    "@babel/core": "^7.1.6",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/plugin-transform-runtime": "^7.1.0",
    "@babel/preset-env": "^7.1.6",
    "@babel/runtime": "^7.1.5",
    "@babel/runtime-corejs2": "^7.1.5",
    "babel-loader": "^8.0.4",
    "babel-minify-webpack-plugin": "^0.3.1",
    "clean-webpack-plugin": "^0.1.19",
    "copy-webpack-plugin": "^4.6.0",
    "css-loader": "^1.0.1",
    "csv-loader": "^3.0.2",
    "es6-promise": "^4.2.5",
    "exports-loader": "^0.7.0",
    "file-loader": "^2.0.0",
    "html-webpack-plugin": "^3.2.0",
    "http-proxy-middleware": "^0.19.0",
    "imports-loader": "^0.8.0",
    "less": "^3.8.1",
    "less-loader": "^4.1.0",
    "mini-css-extract-plugin": "^0.4.4",
    "open": "^0.0.5",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "prettier": "^1.16.4",
    "style-loader": "^0.23.1",
    "vue": "^2.6.8",
    "vue-loader": "^15.7.0",
    "vue-router": "^3.0.2",
    "vue-template-compiler": "^2.6.8",
    "vuex": "^3.1.0",
    "webpack": "^4.29.6",
    "webpack-cli": "^3.1.2",
    "webpack-dev-middleware": "^3.4.0",
    "webpack-dev-server": "^3.1.10",
    "webpack-merge": "^4.1.4",
    "xml-loader": "^1.2.1"
  }
}
